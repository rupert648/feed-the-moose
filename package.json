{
  "name": "feed-the-moose",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "wrangler pages dev .svelte-kit/cloudflare",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "deploy": "wrangler deploy",
    "deploy:cron": "wrangler deploy --config cron-worker/wrangler.toml",
    "deploy:all": "pnpm run deploy && pnpm run deploy:cron",
    "cf-typegen": "wrangler types",
    "db:migrate": "wrangler d1 execute feed-the-moose-db --file=migrations/0001_initial.sql --local",
    "db:migrate:prod": "wrangler d1 execute feed-the-moose-db --file=migrations/0001_initial.sql --remote",
    "db:clear": "node scripts/clear-db.js",
    "db:clear:prod": "node scripts/clear-db.js --remote",
    "db:studio": "wrangler d1 execute feed-the-moose-db --local --command 'SELECT 1'",
    "setup": "pnpm install && pnpm db:migrate",
    "generate-vapid": "node scripts/generate-vapid.js"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250109.0",
    "@sveltejs/adapter-cloudflare": "^6.0.0",
    "@sveltejs/kit": "^2.15.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "wrangler": "^3.99.0"
  },
  "dependencies": {
    "@pushforge/builder": "^1.1.2"
  },
  "type": "module",
  "engines": {
    "node": ">=20"
  }
}
